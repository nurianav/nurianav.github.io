@misc{gregory_b_sadler_martin_2018,
	title = {Martin {Heidegger}, {Being} and {Time} {\textbar} {The} {Ontic} and the {Ontological} {\textbar} {Philosophy} {Core} {Concepts}},
	url = {https://www.youtube.com/watch?v=FE9lcWRgTOE},
	abstract = {Get Heidegger's Being and Time - https://amzn.to/2IvzAnX
Support my work here - https://www.patreon.com/sadler
Philosophy tutorials - https://reasonio.wordpress.com/tutori...

@article{wei_native_2023,
	title = {Native language differences in the structural connectome of the human brain},
	volume = {270},
	issn = {1053-8119},
	url = {https://www.sciencedirect.com/science/article/pii/S1053811923001015},
	doi = {10.1016/j.neuroimage.2023.119955},
	abstract = {Is the neuroanatomy of the language structural connectome modulated by the life-long experience of speaking a specific language? The current study compared the brain white matter connections of the language and speech production network in a large cohort of 94 native speakers of two very different languages: an Indo-European morphosyntactically complex language (German) and a Semitic root-based language (Arabic). Using high-resolution diffusion-weighted MRI and tractography-based network statistics of the language connectome, we demonstrated that German native speakers exhibited stronger connectivity in an intra-hemispheric frontal to parietal/temporal dorsal language network, known to be associated with complex syntax processing. In comparison, Arabic native speakers showed stronger connectivity in the connections between semantic language regions, including the left temporo-parietal network, and stronger inter-hemispheric connections via the posterior corpus callosum connecting bilateral superior temporal and inferior parietal regions. The current study suggests that the structural language connectome develops and is modulated by environmental factors such as the characteristic processing demands of the native language.},
	urldate = {2023-10-22},
	journal = {NeuroImage},
	author = {Wei, Xuehu and Adamson, Helyne and Schwendemann, Matthias and Goucha, Tomás and Friederici, Angela D. and Anwander, Alfred},
	month = apr,
	year = {2023},
	keywords = {Arabic, Cross-linguistic, Diffusion MRI, German, Human brain, Language connectome, Structural connectivity, Tractography},
	pages = {119955},
}


	year = {1995},
	note = {ADS Bibcode: 1995A\&A...297..727B},
	keywords = {POST-AGB, STARS: AGB, STARS: EVOLUTION, STARS: MASS LOSS},
	pages = {727},
}

@article{ventura_dust_2012,
